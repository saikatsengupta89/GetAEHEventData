{
  "version": "2.0",
  "extensions": {
    "eventHubs": {
        "batchCheckpointFrequency": 5,
        "eventProcessorOptions": {
            "maxBatchSize": 256,
            "prefetchCount": 512
        },
        "initialOffsetOptions": {
            "type": "fromStart",
            "enqueuedTimeUtc": ""
        }
    }
  },
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[1.*, 2.0.0)"
  }
}